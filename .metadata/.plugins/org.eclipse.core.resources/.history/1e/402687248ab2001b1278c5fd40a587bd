<script>
	//로그인을 유도하는 검사
    function Warning_check(){ 
    	  var id = '<%=session.getAttribute("vo")%>';
          if(id == "null"){ 
              alert("로그인이 필요한 서비스입니다. 회원이 아니시라면 회원가입후 저희 숨밥서비스를 이용부탁드립니다^^"); 
          }
          else{
             location.replace("/sumbab/mypage/noticePage");
          }
    }   
	//================================================================================
    function Pick_check(){ 
        var id = '<%=session.getAttribute("vo")%>';
        console.log("id");

         if(id=="null"){ 
            alert("로그인이 필요한 서비스입니다. 회원이 아니시라면 회원가입후 저희 숨밥서비스를 이용부탁드립니다^^"); 
         }
         else{
            location.replace("/sumbab/Pick/pickMain");
         }
   }   
    function Mypage_check(){ 
        var id = '<%=session.getAttribute("vo")%>';
        console.log("id");

         if(id=="null"){ 
            alert("로그인이 필요한 서비스입니다. 회원이 아니시라면 회원가입후 저희 숨밥서비스를 이용부탁드립니다^^"); 
         }
         else{
            location.replace("/sumbab/mypage/mypageMain");
         }
   }   
    function Logout_check(){ 
        var id = '<%=session.getAttribute("vo")%>';
        console.log("id");

         if(id=="null"){ 
            alert("로그인이 필요한 서비스입니다. 회원이 아니시라면 회원가입후 저희 숨밥서비스를 이용부탁드립니다^^"); 
         }
         else{
            location.replace("/sumbab/login/logout");
         }
   }      
</script>




	<main class="form-signin">
	<form>
